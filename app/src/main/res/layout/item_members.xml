<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.google.android.material.textview.MaterialTextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="Sebagai apa"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textSize="12sp"
        android:layout_marginStart="5dp"
        android:textStyle="bold"
        android:textColor="@android:color/black"
        android:id="@+id/member_team_research"/>

    <com.google.android.material.textview.MaterialTextView
        android:layout_width="0dp"
        android:padding="5dp"
        android:textAlignment="textEnd"
        android:layout_marginStart="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/member_team_research"
        app:layout_constraintBottom_toBottomOf="@id/member_team_research"
        app:layout_constraintTop_toTopOf="@id/member_team_research"
        android:text="Anggota Peneliti"
        android:textSize="12sp"
        android:textColor="@android:color/black"
        android:id="@+id/value_member_team_research"/>

</androidx.constraintlayout.widget.ConstraintLayout>